{
  "name": "@bundle/compress",
  "version": "0.0.0",
  "access": "public",
  "type": "module",
  "description": "Compression toolkit for `@bundle/core`, supports gzip, base64, lz4, brotli, and zstd.",
  "typedocMain": "src/index.ts",
  "legacy": "lib/index.cjs",
  "main": "lib/index.cjs",
  "browser": "lib/index.js",
  "module": "lib/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./lib/index.d.cts",
        "default": "./lib/index.cjs"
      },
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      }
    },
    "./lib/*": "./lib/*",
    "./src/*": "./src/*",
    "./deno/*": "./src/deno/*",
    "./package.json": "./package.json"
  },
  "directories": {
    "lib": "./lib",
    "src": "./src"
  },
  "files": [
    "lib",
    "src"
  ],
  "publishConfig": {
    "provenance": true
  },
  "scripts": {
    "build:wasm": "tsx ./scripts/build-wasm.ts",
    "build": "tsup; find ./src -name '*.wasm' | xargs -I {} cp {} ./lib",
    "test:run": "vitest run && pnpm repl; pnpm deno",
    "test": "vitest",
    "repl": "tsx repl.ts",
    "deno": "deno run --allow-net --allow-env --allow-read --allow-write --allow-run ./repl.ts",
    "pre-release": "pnpm build; pnpm test:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/okikio/bundlejs.git",
    "directory": "packages/compress"
  },
  "keywords": [
    "bundlejs",
    "deno",
    "gzip",
    "brotli",
    "lz4",
    "zstd",
    "compress",
    "typescript"
  ],
  "author": {
    "name": "Okiki Ojo",
    "email": "hey@okikio.dev",
    "url": "https://okikio.dev"
  },
  "bugs": {
    "url": "https://github.com/okikio/bundlejs/issues"
  },
  "homepage": "https://bundlejs.com",
  "dependencies": {
    "@bundle/utils": "workspace:^"
  },
  "peerDependencies": {
    "@bundle/utils": "^"
  },
  "devDependencies": {
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "vitest": "^3.0.7"
  }
}