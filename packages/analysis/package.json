{
  "name": "@bundle/analysis",
  "version": "0.0.0",
  "access": "public",
  "type": "module",
  "description": "An animation library for the modern web which utilizes the Web Animation API (WAAPI) to create butter smooth animation.",
  "typedocMain": "src/index.ts",
  "types": "@types/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./lib/index.d.cts",
        "default": "./lib/index.cjs"
      },
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      }
    },
    "./lib/*": "./lib/*",
    "./types/*": "./@types/*",
    "./@types/*": "./@types/*",
    "./src/*": "./src/*"
  },
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "vite build",
    "test:run": "vitest run && pnpm deno",
    "test": "vitest",
    "repl": "tsx repl.ts",
    "deno": "deno run --allow-net --allow-env --allow-read --allow-write --allow-run ./repl.ts",
    "pre-release": "pnpm build && pnpm test:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/okikio/bundlejs.git",
    "directory": "packages/analysis"
  },
  "keywords": [
    "modern",
    "bundlejs",
    "bundle size checker",
    "esbuild-wasm",
    "gzip",
    "brotli",
    "deno",
    "bundler",
    "treeshake",
    "bundlephobia",
    "cdn",
    "github",
    "skypack",
    "unpkg",
    "compressed",
    "jsx",
    "minify",
    "typescript",
    "bundle analysis",
    "package analyzer"
  ],
  "author": {
    "name": "Okiki Ojo",
    "email": "hey@okikio.dev",
    "url": "https://okikio.dev"
  },
  "bugs": {
    "url": "https://github.com/okikio/bundlejs/issues"
  },
  "homepage": "https://bundlejs.com",
  "dependencies": {
    "@bundle/compress": "workspace:^",
    "@bundle/utils": "workspace:^",
    "bytes": "^3.1.2",
    "d3": "^7.9.0",
    "esbuild-wasm": "^0.23.0",
    "nanoid": "^5.0.7",
    "solid-js": "^1.8.20",
    "webcola": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/bytes": "^3.1.4",
    "@types/d3": "^7.4.3",
    "jsdom": "^24.1.1",
    "tsup": "^8.2.4",
    "tsx": "^4.17.0",
    "vite": "^5.4.0",
    "vite-plugin-solid": "^2.10.2",
    "vitest": "^2.0.5"
  }
}